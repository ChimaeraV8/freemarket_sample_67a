.mypage-contents__inner
  .mypage-contents__user
    %h1.user-name
      = @user
    %p.exhibitions
      出品数: 0
  .mypage-contents__message
    .mypage-contents__message-tabs
      .mypage-contents__message-tabs__tab
        =link_to '#' , class:'hover'do
          お知らせ
      .mypage-contents__message-tabs__tab--right
        =link_to '#' , class:'hover'do
          やることリスト
    .mypage-contents__message__contents-box
      .mypage-contents__message__contents-box__nontification-one
        .mypage-contents__message__contents-box__nontification-one__image
          = image_tag 'pict/pict-reason-01.jpg',size:'48x48'
        .mypage-contents__message__contents-box__nontification-one__message
          %p
            = link_to '#' , class:'hover'do
              事務所からのメッセージ
          %ul.abc
            = link_to '#' , class:'hover'do
              %li
              =link_to '#' , class:'hover' do
                🕙
              %li
              =link_to '#' , class:'hover'do
                7日前
        .mypage-contents__message__contents-box__nontification-one__icon-arrow-right
          ›
    .mypage-contents__message__contents-box
      .mypage-contents__message__contents-box__footer
        =link_to '#' , class:'hover'do
    .mypage-contents__tab__contents-box
      .mypage-contents__tab__contents-box__nontification-one
        .mypage-contents__tab__contents-box__nontification-one__image
          = image_tag 'pict/pict-reason-01.jpg',size:'48x48'
        .mypage-contents__tab__contents-box__nontification-one__message
          %ul
            事務所からのメッセージ
            %li
              🕙
            %li
              7日前
        .mypage-contents__tab__contents-box__nontification-one__icon-arrow-right
          ›

    .mypage-contents__tab__contents-box
      .mypage-contents__tab__contents-box__nontification-one
        .mypage-contents__tab__contents-box__nontification-one__inner
          .mypage-contents__tab__contents-box__nontification-one__inner__image
            = image_tag 'pict/pict-reason-01.jpg',size:'48x48'
          .mypage-contents__tab__contents-box__nontification-one__inner__box
            .mypage-contents__tab__contents-box__nontification-one__inner__box__message
              事務所からのメッセージ
            .mypage-contents__tab__contents-box__nontification-one__inner__box__timebox
              .mypage-contents__tab__contents-box__nontification-one__inner__box__timebox__icon
                🕙
              .mypage-contents__tab__contents-box__nontification-one__inner__box__timebox__days
                7日前
          .mypage-contents__tab__contents-box__nontification-one__inner__icon-arrow-right
            ›
    .mypage-contents__tab__contents-box
      .mypage-contents__tab__contents-box__footer
        =link_to '#' do
          一覧を見る

  .mypage-contents__container
    .mypage-contents__container-tab
      .mypage-contents__container-tab_message
        購入した商品
    .mypage-contents__container-tabs
      .mypage-contents__container-tabs__tab
        =link_to '#' , class:'hover'do
          取引中
      .mypage-contents__container-tabs__tab--right
        =link_to '#' , class:'hover'do
          過去の取引
    .mypage-contents__container__contents-box
      .mypage-contents__container__contents-box__nontification-one
        .mypage-contents__container__contents-box__nontification-one__image
          = image_tag 'pict/pict-reason-01.jpg',size:'48x48'
        .mypage-contents__container__contents-box__nontification-one__message
          %ul
            = link_to '#',class: 'hover' do
              ○○を購入しました。受取評価をお願いします。
            %li.abc
              = link_to '#',class:'backcolor-red' do
                受取評価待ち
        .mypage-contents__container__contents-box__nontification-one__icon-arrow-right
          ›
          
    .mypage-contents__container__contents-box
      .mypage-contents__container__contents-box__footer
        =link_to '#' , class:'hover'do
          一覧を見る
